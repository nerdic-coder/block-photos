    "electron-forge": "5.2.2",
    "electron-prebuilt-compile": "2.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "jest-enzyme": "^6.0.2",
    "json-loader": "^0.5.7",
    "path": "^0.12.7",
    "style-loader": "^0.21.0",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
