        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      }
    }
  },
  "dependencies": {
    "blockstack": "18.0.2",
    "cors": "^2.8.4",
    "electron-compile": "6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.16.3",
    "localforage": "1.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "uniqid": "5.0.3"
  },
  "devDependencies": {
    "@ionic/core": "4.0.0-beta.0",
    "@types/react": "16.4.7",
    "ajv": "6.5.2",
